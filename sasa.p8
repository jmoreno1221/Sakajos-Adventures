pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
---runs the game starting with the title screen
function _init()
	titleinit()
end

---creates all variables for the game
function gameinit()
	mode=1
	px=20    -- x-position
	py=496   -- y-position
	pstate=0 -- current player state
	pspr=1   -- current sprite
	pdir=0   -- current direction
	pat=0    -- player state timer
	lvl=0
	camx=0
	camy=384
end

---enables mode to be at 0 which where
---our update() function below will be called
function titleinit()
	mode=0
end

---the update loop to either run the title screen
---or begin to run the game_update60() function
function _update()
	if (mode==0) then
		titleupdate()
	else
		game_update60()
	end
end

---begins our draw function to draw all of our sprites
function _draw()
	-- clears screen
	cls()
	-- places camera at 1st level
	camera(camx, camy)
	-- draw the world
	map(0,0,0,0,128,512)
	-- draw the player
	spr(pspr,px,py,1,1,pdir==-1)
	-- title screen
	print("press z to start", 32, 64, 7)
end

---this is where our camera moves when the player
---advances throughout the levels
function update_camera(lvl)
	if lvl==0 then
		if py < 390 and camy>0 then
			camy = 264
		else
			camy = 384
		end
	end
end

---changes the state of the player to know
---which sprite to draw when the player is
---performing any action
function change_state(s)
	pstate=s
	pat=0
end

function can_left()
 l=mget(flr((px-1)/8),flr((py)/8))
 return not fget(l,1)
end

function can_right()
 r=mget(flr((px+8)/8),flr((py)/8))
 return not fget(r,1)
end

function can_jump()
 j=mget(px,flr(py-1)/8)
 return not fget(j,0)
end

function canfall()
 -- get the map tile under the player
 v=mget(flr((px+2)/8),flr((py+8)/8))
 -- see if its flagged as wall
 return not fget(v,0)
end

function titleupdate()
print("press z to start", 32, 64, 7)
if btn(4,0) then
 gameinit()
end

end
function game_update60()
 left=btn(â¬…ï¸) -- button0 state
 right=btn(âž¡ï¸) -- button1 state
 jump=btn(ðŸ…¾ï¸) -- button2 state
 shoot=btn(âŽ)

 px%=128   -- no bounds
 pat+=1    -- increment timer

 -- idle state
 if pstate==0 then
   pspr=0      -- idle sprite
   if (left or right) change_state(1)
   if (jump) then
     change_state(3)
     sfx(01)
   end
   if (canfall()) change_state(2)
 end
 
 -- walk state
 if pstate==1 then
   if (pat%3==0) then sfx(0) end -- sound
   if (left and can_left()) then -- set direction
     pdir=-1
   elseif (left and jump and can_left()) then
     pdir=-1
     sfx(01)
   elseif (right and can_right()) then -- set direction
     pdir=1
   elseif (right and jump and can_right()) then
     pdir=1
     sfx(01)
   else pdir=0
   end
   
   if (jump) then
     change_state(3)
     sfx(01)
   end
   if (canfall()) change_state(2)
   -- move the player
   px+=(pdir*.75)*min(pat,2)
   pspr=flr(pat/2)%2
   if (not (left or right)) change_state(0)
 end

 -- fall state
 if pstate==2 then
   	pspr=2
   if (canfall()) then
   if (left and can_left()) px-=1
   if (right) px+=1
   	py+=min(4,pat)
   if (not canfall()) py=flr(py/8)*8
   else 
    py=flr(py/8)*8
    sfx(2) -- ground contact sfx
   	change_state(0)
   end
 end
 
 if (shoot) then
 bullets(px,py)
 end

 -- jump state
 if pstate==3 then
   if (pat==1) sfx(0) -- play sfx
   pspr=2
   py-=7-pat
   if (left and can_left()) px-=2 pdir=-1
   if (right) px+=2 pdir=1
   if (not jump or pat>7) then
     if (not canfall()) py=py-py%8
     change_state(0)
   end
 end
 update_camera(lvl)
end

function bullets(x,y)
spr(4,x,y)
end

