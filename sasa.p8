version 16
__lua__
function _init()
x=50
y=50
end

function _update()
if btn(0) then x = x-1 end
if btn(1) then x = x+1 end
if btn(2) then y = y-1 end
if btn(3) then y = y+1 end
end

function _draw()
cls()
spr(1,x,y)
map(0,0,0,0,127,127)
end
