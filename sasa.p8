pico-8 cartridge // http://www.pico-8.com
version 16
__lua__

function _init()
playerx=20
playery=496
camx=0
camy=384
lvl = 0
end

function _update()
if btn(0) then 
playerx = playerx-1 
end
if btn(1) then
playerx = playerx+1
end
if btn(2) then
playery = playery-1
end
if btn(3) then
playery = playery+1
end
if btn(4) then lvl=1 end
if btn(5) then lvl=0 end
update_camera()
end

function _draw()
cls()
camera(camx,camy)
draw_background(lvl)
map(0,0,0,0,128,512)
draw_player()
camera()

end

function draw_background(lvl)
if lvl==0 then
rectfill(0,0,127,127,12)
elseif lvl==1 then
rectfill(0,0,127,127,13)
end
end

function draw_player()
spr(1,playerx,playery)
end

function update_camera()
if playery < 448 and camy>0 then
camy-=0.5
end
end
