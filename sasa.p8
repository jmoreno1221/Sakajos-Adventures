function _update()
if (mode==0) then
	titleupdate()
else


 if (score%100==0) then
 --every 100 points these things happen
 bgcolor+=1
 score+=10
 level+=1
 blue_speed+=0.02
 red_speed+=0.02
 end
 -----------------------------
 
 
 t+=1
 control()
 if moving then
  mt+=1
  foreach(ents,upe)
  if #bads<8+(mt/100) and mt%8==0 then
   sbad()
  end  
 end
 if hero.death and btnp(5) then
  game_init()
 end
end
end
-->8
function titleupdate()
cls()
if btnp(1,0) then cls() title_char+=1 end
if btnp(0,0) then cls() title_char-=1 end
if (title_char < 48) then title_char = 54 end
if (title_char > 54) then title_char = 48 end
spr(title_char,45,100)
--add new character names
local names={"sakajo", "sa", "ka", "jo", "brandon", "dr.g", "dr.b"}
print(names[title_char-47],65,102,7)
if btn(5,0) then
	game_init()
end
end

