function titledraw()
--print("hello, player",37,36,7)
spr(64,16,48,12,2)
spr(76,46,57,3,1)
spr(92,46,65,3,1)
spr(96,7,74,14,2)
spr(1,9,66,1,1)
print("use ⬅️ ⬇️ ➡️ ⬆️ keys to move", 10, 20, 7)
print("use `b' to shoot", 32, 30, 7)
print("select your player", 30, 93, 7)
print("<", 40, 102, 7)
print(">", 55, 102, 7)
print("press `a' to begin",30,120,7)
end

